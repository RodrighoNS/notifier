<% content_for :title, "Email channels" %>

<h1>Email channels</h1>

<div id="email_channels">
  <% @email_channels.each do |email_channel| %>
    <%= render email_channel %>
    <p>
      <%= link_to "Show this email channel", email_channel %>
    </p>
  <% end %>
</div>

<%= link_to "New email channel", new_email_channel_path %>
